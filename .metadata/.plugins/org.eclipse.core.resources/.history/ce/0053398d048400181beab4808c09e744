package com.java.practise;

public class Pattern_2 {

	public static void main(String[] args) {
		
		int num = 5;
		
		for (int i = 0; i <=num; i++) {
			
			int n=5;
			for (int j = 0; j <=num; j++) {
				
				if (i>=j) {
					System.out.print(num--);
				} 
				else {
					System.out.print("*");
				}
				
			}
			System.out.println();
		}
		
		

	}

}

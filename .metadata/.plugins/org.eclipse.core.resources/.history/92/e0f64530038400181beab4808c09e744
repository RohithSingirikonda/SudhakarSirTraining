package com.java.practise;

public class Pattern {

	public static void main(String[] args) {
		
		int num = 5;
		
		for (int i = 1; i <=num; i++) {
			
			int n = 1;
			
			for (int j = 1; j <=(2*i)-1; j++) {
				
				if (i>j) {
					System.out.print(++n);
				}
				
				else if (i==j) {
					System.out.print(n);
				}
				
				else {
					n--;
					System.out.print(n);
				}
			}
			System.out.println();
			
		}	

	}

}

package com.java.practise;

public class Pattern_2 {

	public static void main(String[] args) {
		
		int num = 6;
		
		for (int i = 1; i <=num; i++) {
			
			int n=6;
			for (int j = 1; j <=num; j++) {
				
				if (i>=j) {
					System.out.print(n--);
				} 
				else {
					System.out.print("*");
				}
				
			}
			System.out.println();
		}
		
		

	}

}

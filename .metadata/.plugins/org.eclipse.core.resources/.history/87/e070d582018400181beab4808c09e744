package com.java.practise;

public class Fibonacci_Series_Using_forloop {

	public static void main(String[] args) {
		
		int value = 5;
		int f0=0, f1=1, f2 ;
		
		System.out.print(f0+" "+f1);
	
		for (int i = 2; i < value; i++) {
			
			f2 = f0 + f1;
			f0 = f1;
			f1 = f2;
			System.out.print(" "+f2);
			
			
		}
		
	}

}

package com.java.practise;

public class Pattern {

	public static void main(String[] args) {
		
		int num = 5, n = 1;
		
		for (int i = 1; i <=num; i++) {
			
			for (int j = 0; j < (2*i)-1; j++) {
				
				if (i>j) {
					System.out.println(n++);
				}
				else if (i==j) {
					System.out.println(n);
				}
				else {
					n--;
					System.out.println(n);
				}
			}
			System.out.println();
			
		}
		
		

	}

}

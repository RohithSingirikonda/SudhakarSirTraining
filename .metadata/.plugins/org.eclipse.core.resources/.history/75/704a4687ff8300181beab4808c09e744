package com.java.practise;

public class Fibonacci_Series {

	public static void main(String[] args) {
		
		int value = 5;
		int f0=0, f1=1, f2 ;
		
		
		for (int i = 0; i < value; i++) {
			System.out.print(f0);
			System.out.print(f1);
			int temp;
			f2 = f0 + f1;
			
			temp = f0;
			f0 = f1;
			f1 = f2;
			System.out.print(f2);
			
			
		}
		
		

	}

}
